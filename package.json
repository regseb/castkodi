{
  "name": "castkodi",
  "version": "7.5.0",
  "description": "WebExtension pour diffuser des vidéos et des musiques sur Kodi.",
  "keywords": [
    "cast",
    "kodi",
    "webextension",
    "chrome",
    "firefox",
    "youtube",
    "twitch",
    "vimeo",
    "soundcloud"
  ],
  "homepage": "https://github.com/regseb/castkodi#readme",
  "bugs": {
    "url": "https://github.com/regseb/castkodi/issues",
    "email": "regseb@gmail.com"
  },
  "license": "MIT",
  "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
  "funding": "https://www.paypal.me/sebastienregne",
  "repository": "regseb/castkodi",
  "type": "module",
  "scripts": {
    "postinstall": "node .script/sync.js",
    "lint": "node .script/lint.js",
    "lint:fix": "node .script/lint.js --fix",
    "lint:types": "tsc --project .tsconfig_lint.json",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --config test/unit/mocharc.json",
    "test:integration": "mocha --config test/integration/mocharc.json",
    "test:coverage": "stryker run",
    "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json",
    "switch:chromium": "node .script/switch.js chromium",
    "switch:firefox": "node .script/switch.js firefox",
    "start:chromium": "node .script/start.js chromium",
    "start:firefox": "node .script/start.js firefox",
    "build": "node .script/build.js",
    "clean": "node .script/clean.js"
  },
  "dependencies": {
    "linkedom": "0.16.5"
  },
  "devDependencies": {
    "@prantlf/jsonlint": "14.0.3",
    "@prettier/plugin-xml": "3.2.2",
    "@stryker-mutator/core": "8.0.0",
    "@stryker-mutator/mocha-runner": "8.0.0",
    "@types/firefox-webext-browser": "120.0.0",
    "@types/jsdom": "21.1.6",
    "@types/mocha": "10.0.6",
    "@types/sinon": "17.0.2",
    "addons-linter": "6.19.0",
    "eslint": "8.55.0",
    "eslint-plugin-array-func": "4.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-mocha": "10.2.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.1.2",
    "eslint-plugin-unicorn": "49.0.0",
    "htmlhint": "1.1.4",
    "jsdom": "23.0.1",
    "markdownlint": "0.32.1",
    "metalint": "0.14.1",
    "mocha": "10.2.0",
    "mock-socket": "9.3.1",
    "npm-package-json-lint": "7.1.0",
    "prettier": "3.1.0",
    "purgecss": "5.0.0",
    "sinon": "17.0.1",
    "stylelint": "16.0.1",
    "stylelint-order": "6.0.4",
    "typedoc": "0.25.4",
    "typescript": "5.3.3",
    "web-ext": "7.9.0",
    "yaml-lint": "1.7.0"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "private": true
}
