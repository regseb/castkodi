{
  "name": "castkodi",
  "version": "7.13.0",
  "description": "WebExtension pour diffuser des vidéos et des musiques sur Kodi.",
  "keywords": [
    "cast",
    "kodi",
    "webextension",
    "chrome",
    "firefox",
    "youtube",
    "twitch",
    "vimeo",
    "soundcloud"
  ],
  "homepage": "https://github.com/regseb/castkodi#readme",
  "bugs": {
    "url": "https://github.com/regseb/castkodi/issues",
    "email": "regseb@gmail.com"
  },
  "license": "MIT",
  "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
  "funding": [
    "https://buymeacoffee.com/regseb",
    "https://www.paypal.me/sebastienregne"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regseb/castkodi.git"
  },
  "type": "module",
  "scripts": {
    "postinstall": "node .script/sync.js",
    "lint": "metalint",
    "lint:fix": "metalint --fix",
    "lint:types": "tsc --project .tsconfig_lint.json",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --config test/unit/mocharc.json",
    "test:integration": "mocha --config test/integration/mocharc.json",
    "test:coverage": "stryker run",
    "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json",
    "start:chromium": "web-ext run --source-dir src/ --target chromium",
    "start:firefox": "web-ext run --source-dir src/ --target firefox-desktop",
    "start:firefox-android": "web-ext run --source-dir src/ --target firefox-android --firefox-apk org.mozilla.fenix",
    "build": "node .script/build.js",
    "clean": "node .script/clean.js"
  },
  "dependencies": {
    "linkedom": "0.18.12"
  },
  "devDependencies": {
    "@prantlf/jsonlint": "16.0.0",
    "@prettier/plugin-xml": "3.4.2",
    "@stryker-mutator/core": "9.1.1",
    "@stryker-mutator/mocha-runner": "9.1.1",
    "@types/firefox-webext-browser": "120.0.4",
    "@types/jsdom": "21.1.7",
    "@types/mocha": "10.0.10",
    "addons-linter": "8.0.0",
    "eslint": "9.36.0",
    "eslint-plugin-array-func": "5.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "60.3.1",
    "eslint-plugin-mocha": "11.1.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-no-unsanitized": "4.1.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-unicorn": "61.0.2",
    "globals": "16.4.0",
    "htmlhint": "1.7.1",
    "jsdom": "27.0.0",
    "markdownlint": "0.38.0",
    "metalint": "0.20.0",
    "mocha": "11.7.2",
    "mock-socket": "9.3.1",
    "npm-package-json-lint": "9.0.0",
    "prettier": "3.6.2",
    "purgecss": "7.0.2",
    "stylelint": "16.24.0",
    "stylelint-order": "7.0.0",
    "typedoc": "0.28.13",
    "typescript": "5.9.2",
    "urlpattern-polyfill": "10.1.0",
    "web-ext": "8.10.0",
    "yaml-lint": "1.7.0"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "private": true
}
