{
    "applications": {
        "gecko": {
            "id": "castkodi@regseb.github.io",
            "strict_min_version": "84.0"
        }
    },
    "manifest_version": 2,
    "name": "Cast Kodi",
    "version": "5.9.0",
    "description": "__MSG_manifest_description__",
    "icons": {
        "48": "img/icon.svg",
        "96": "img/icon.svg"
    },
    "browser_action": {
        "browser_style": false,
        "default_icon": "img/icon.svg",
        "default_title": "Cast Kodi",
        "default_popup": "popup/index.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+K"
            }
        }
    },
    "author": "Sébastien Règne",
    "background": {
        "page": "background/index.html"
    },
    "default_locale": "en",
    "homepage_url": "https://github.com/regseb/castkodi",
    "optional_permissions": [
        "bookmarks",
        "history"
    ],
    "options_ui": {
        "browser_style": false,
        "page": "options/index.html"
    },
    "permissions": [
        "<all_urls>",
        "activeTab",
        "menus",
        "notifications",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ]
}
